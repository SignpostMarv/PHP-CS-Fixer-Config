<?xml version="1.0"?>
<psalm
    name="Example Psalm config with recommended defaults"
    stopOnFirstError="false"
    useDocblockTypes="true"
    totallyTyped="true"
>
    <projectFiles>
        <file name=".php_cs.dist" />
        <directory name="src" />
    </projectFiles>

    <issueHandlers>
        <UnusedClass errorLevel="info" />
    </issueHandlers>
</psalm>
